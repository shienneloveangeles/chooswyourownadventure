#include <linkbot.h>
CLinkbotI robotSignal, robotRacer;
string_t color;
robotRacer.setSpeed(4,2);
robotSignal.setLEDColor("green");
robotRacer.driveDistance(20,2);
robotSignal.setLEDColor("red");
robotRacer.delaySeconds(4);
robotSignal.setLEDColor("green");
robotRacer.driveDistance(11,2);
robotSignal.setLEDColor("red");
